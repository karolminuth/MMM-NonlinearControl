<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GeneralController">
	<top>
		<HBox fx:id="hBox" alignment="TOP_CENTER" prefHeight="144.0"
			prefWidth="600.0" BorderPane.alignment="CENTER">
			<children>
				<Label fx:id="tekstOpisuPobudzenia" alignment="BOTTOM_CENTER"
					contentDisplay="CENTER" prefHeight="71.0" prefWidth="173.0"
					text="Rodzaje&#10;pobudzenia" textAlignment="CENTER" />
				<VBox fx:id="vBox1" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="333.0"
					HBox.hgrow="ALWAYS">
					<children>
						<Label fx:id="labelProstokat" alignment="CENTER"
							contentDisplay="CENTER" text="Fala prostokątna">
							<VBox.margin>
								<Insets left="10.0" top="10.0" />
							</VBox.margin>
						</Label>
						<ImageView fx:id="imageProstokat" fitHeight="150.0"
							fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../impulsy/prostokat.jpg" />
							</image>
						</ImageView>
					</children>
				</VBox>
				<VBox fx:id="vBox2" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="333.0"
					HBox.hgrow="ALWAYS">
					<children>
						<Label fx:id="labelTrojkat" alignment="CENTER"
							contentDisplay="CENTER" text="Fala trójkątna">
							<VBox.margin>
								<Insets left="10.0" top="10.0" />
							</VBox.margin>
						</Label>
						<ImageView fx:id="imageTrojkat" fitHeight="150.0"
							fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../impulsy/trojkat.jpg" />
							</image>
						</ImageView>
					</children>
				</VBox>
				<VBox fx:id="vBox3" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="333.0"
					HBox.hgrow="ALWAYS">
					<children>
						<Label fx:id="labelSinusoida" alignment="CENTER"
							contentDisplay="CENTER" text="Sinusoida">
							<VBox.margin>
								<Insets left="10.0" top="10.0" />
							</VBox.margin>
						</Label>
						<ImageView fx:id="imageSinusoida" fitHeight="150.0"
							fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../impulsy/sinusoida.jpg" />
							</image>
						</ImageView>
					</children>
				</VBox>
			</children>
		</HBox>
	</top>
	<left>
		<VBox fx:id="vBoxDane" alignment="CENTER" prefHeight="200.0"
			prefWidth="100.0" BorderPane.alignment="CENTER">
			<children>
				<Label fx:id="labela" text="Wartość a:" />
				<TextField fx:id="textFielda" />
				<Label fx:id="labelA" text="Wartość A:" />
				<TextField fx:id="textFieldA" />
				<Label fx:id="labelT" text="Wartość T:" />
				<TextField fx:id="textFieldT" />
				<Button fx:id="buttonUstawDane" text="Ustaw">
					<VBox.margin>
						<Insets top="30.0" />
					</VBox.margin>
				</Button>
			</children>
		</VBox>
	</left>
	<center>
		<VBox fx:id="vBoxWykresy" prefHeight="200.0" prefWidth="100.0"
			BorderPane.alignment="CENTER" />
	</center>
</BorderPane>
